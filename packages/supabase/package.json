{
  "name": "@canny_ecosystem/supabase",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome lint",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "seed": "tsx src/seed/index.ts",
